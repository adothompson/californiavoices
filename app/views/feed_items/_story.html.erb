<% story = feed_item.item %>
<div class="feed_item photo_feed" id="<%= feed_item.dom_id %>">
  <div class="left">
    <img src="/images/blurp_photos.png" class="png icon_small" title="User Photo Icon"  alt="User Photo Icon" /> 
    <%= link_to h(story.profile.f), story.profile %> added a new story <%= link_to h(story.title), story_path(story) %>
  </div> 
  <div class="right">
    <%= x_feed_link(feed_item) if me %>
  </div>
  <div class="clear"></div>
</div>
